<view class="button_0" >   
    <button class="button_0" bindtap="select_history_2">查询手动打氧记录</button>
</view>

<view class="table_title">
    <view class="tr bg-w">
        <view class="th_">历史手动打氧时间表</view>
    </view>
</view>  

 <view class="table">
    <block wx:for="{{history}}" >
        <view class="tr bg-g" wx:if="{{index % 2 == 0}}">
            <view class="td">{{item.o2_time}}</view>
        </view>
        <view class="tr" wx:else>
            <view class="td">{{item.o2_time}}</view>
        </view>
    </block>
</view>

