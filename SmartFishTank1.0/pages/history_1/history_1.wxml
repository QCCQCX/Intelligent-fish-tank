<view class="button_0" >   
    <button class="button_0"  bindtap="select_history_1">查询手动投喂记录</button>
 </view>

<view class="table_title">
    <view class="tr bg-w">
        <view class="th_">历史手动投喂时间表</view>
    </view>
</view>  

 <view class="table">
    <block wx:for="{{history}}" >
        <view class="tr bg-g" wx:if="{{index % 2 == 0}}">
            <view class="td">{{item.eat_time}}</view>
        </view>
        <view class="tr" wx:else>
            <view class="td">{{item.eat_time}}</view>
        </view>
    </block>
</view>

